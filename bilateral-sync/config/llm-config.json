{
  "llm_providers": {
    "claude": {
      "name": "Claude 3.5 Sonnet",
      "provider": "anthropic",
      "model": "anthropic/claude-3.5-sonnet",
      "base_url": "https://openrouter.ai/api/v1",
      "api_key_env": "OPENROUTER_API_KEY",
      "max_tokens": 2000,
      "temperature": 0.3,
      "cost_per_1k_tokens": 0.003,
      "capabilities": [
        "excellent reasoning",
        "strong coding",
        "good context understanding",
        "reliable responses"
      ],
      "status": "active",
      "default": true
    },
    "chatgpt5": {
      "name": "ChatGPT 5",
      "provider": "openai",
      "model": "gpt-5",
      "base_url": "https://api.openai.com/v1",
      "api_key_env": "OPENAI_API_KEY",
      "max_tokens": 4000,
      "temperature": 0.3,
      "cost_per_1k_tokens": 0.005,
      "capabilities": [
        "advanced reasoning",
        "excellent coding",
        "superior context understanding",
        "latest knowledge",
        "multimodal capabilities"
      ],
      "status": "premium",
      "requires_subscription": true,
      "subscription_type": "ChatGPT Plus"
    },
    "gpt4": {
      "name": "GPT-4",
      "provider": "openai",
      "model": "gpt-4",
      "base_url": "https://api.openai.com/v1",
      "api_key_env": "OPENAI_API_KEY",
      "max_tokens": 4000,
      "temperature": 0.3,
      "cost_per_1k_tokens": 0.03,
      "capabilities": [
        "strong reasoning",
        "excellent coding",
        "good context understanding",
        "reliable performance"
      ],
      "status": "standard",
      "requires_subscription": false
    },
    "claude_opus": {
      "name": "Claude 3.5 Opus",
      "provider": "anthropic",
      "model": "anthropic/claude-3.5-opus",
      "base_url": "https://openrouter.ai/api/v1",
      "api_key_env": "OPENROUTER_API_KEY",
      "max_tokens": 4000,
      "temperature": 0.3,
      "cost_per_1k_tokens": 0.015,
      "capabilities": [
        "superior reasoning",
        "excellent coding",
        "best context understanding",
        "multimodal capabilities"
      ],
      "status": "premium",
      "requires_subscription": false
    }
  },
  "default_config": {
    "primary_llm": "claude",
    "fallback_llm": "gpt4",
    "max_conversation_length": 8000,
    "auto_switch_threshold": 6000,
    "context_window_size": 10000
  },
  "conversation_management": {
    "enable_auto_summarization": true,
    "summary_threshold": 5000,
    "enable_context_compression": true,
    "compression_threshold": 7000,
    "enable_memory_management": true,
    "memory_retention_days": 30
  },
  "performance_optimization": {
    "enable_streaming": true,
    "enable_caching": true,
    "cache_ttl_seconds": 3600,
    "enable_batch_processing": false,
    "max_batch_size": 5
  },
  "monitoring": {
    "enable_usage_tracking": true,
    "enable_performance_monitoring": true,
    "enable_error_tracking": true,
    "enable_cost_monitoring": true
  },
  "security": {
    "enable_api_key_rotation": false,
    "enable_request_logging": true,
    "enable_response_filtering": true,
    "max_request_size_bytes": 1048576
  }
}

